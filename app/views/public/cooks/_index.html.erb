<% @cooks.each do |cook| %>

  <div class="card-wrapper">
    <div class="card-img">
    <%= link_to(cook) do %>
    <%= image_tag cook.get_image, size: "260x200" %>
    <% end %>
    </div>
    <div class="card-box">
    <h5 class="col-6"><%= link_to cook.name, cook_path(cook.id),class: 'nav-link text-dark' %></h5>

    </div>
    </div>


<% end %>
